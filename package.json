    "jasmine": "^3.8.0",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "prettier-eslint": "^13.0.0",
